<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
  <%= f.button :submit, class: "btn btn-primary mb-2"  %>
<% end %>
<h2><%= link_to "Go Back", restaurants_path, class: "btn btn-primary"%></h2>
